                     

firstR = ms_flux3.Data.RA(1,:);
firstD = ms_flux3.Data.Dec(1,:);
R1 =       firstR(~isnan(firstR))
D1 = firstD(~isnan(firstR))

             
[LC,RA,Dec,Xcat,Ycat] = getCatData(356.436057521198,34.181347341874,Target)

%%
RA_stddev  = []
Dec_stddev = []
ra3 = [];
dec3 = [];


for i = 1 : length(R1)
    
    i
    
    [LC,RA,Dec,Xcat,Ycat] = getCatData(R1(i),D1(i),Target);
    
    
    if sum(~isnan(RA)) > 150
        
        if mean(Xcat,'omitnan') > 150
            if mean(Xcat,'omitnan')< 1600
                if mean(Ycat,'omitnan') > 150
                    if mean(Ycat,'omitnan')< 1600
                
        
                          RA_stddev = [RA_stddev ; std(RA,'omitnan') ] ;
                          Dec_stddev = [Dec_stddev ; std(Dec,'omitnan')];
                          ra3 = [ra3 ; mean(RA,'omitnan')];
                          dec3 = [dec3 ; mean(Dec,'omitnan')];
                    end
                end
            end
        end
    end

    
    
end




RA_sd_arcsec = RA_stddev * 3600;
Dec_sd_arcsec = Dec_stddev * 3600;

subplot(1,2,1)

histogram(RA_sd_arcsec, 'BinWidth', 0.005); 

xlabel('SD [arcsec]','Interpreter','latex');
ylabel('Frequency','Interpreter','latex');
title('Standard Deviation over 220 frames in RA','Interpreter','latex');

% Set x-axis labels
xticks(get(gca, 'xtick'));
xticklabels(sprintfc('%.3f''', get(gca, 'xtick')));  % Convert degrees to arcseconds

subplot(1,2,2)

histogram(Dec_sd_arcsec, 'BinWidth', 0.005); 

xlabel('SD [arcsec]','Interpreter','latex');
ylabel('Frequency','Interpreter','latex');
title(' In Dec','Interpreter','latex');

% Set x-axis labels
xticks(get(gca, 'xtick'));
xticklabels(sprintfc('%.3f''', get(gca, 'xtick')));  % Convert degrees to arcseconds
%%

figure;
subplot(1,2,1)
semilogy(BP1, RA_sd_arcsec, 'o');
xlabel('$\mathrm{B_p}$', 'Interpreter', 'latex');
ylabel('$\mathrm{RA\ [arcsec]}$', 'Interpreter', 'latex');
title(' Standard Deviation in RA vs $B_p$', 'Interpreter', 'latex');
grid on;




