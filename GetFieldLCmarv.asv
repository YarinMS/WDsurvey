if ~rem(Args.Tel,2) 
    
    if Args.Tel == 2 
        
        PathSegment1 = ['/Users/yarinms/marvin/LAST.01.',Args.Mount,'.0',num2str(Args.Tel),'/'];
    else
        PathSegment1 = ['/Users/yarinms/marvin/LAST.01.',Args.Mount,'.0',num2str(Args.Tel),'/'];
        
    end
    
    
    
else
    
    if Args.Tel == 1
